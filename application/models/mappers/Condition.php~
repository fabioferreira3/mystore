<?php



use Doctrine\Mapping as ORM;

/**
 * Condition
 *
 * @Table(name="condition")
 * @Entity
 */
class Condition
{
    /**
     * @var integer $id
     *
     * @Column(name="id", type="integer", nullable=false)
     * @Id
     * @GeneratedValue(strategy="IDENTITY")
     */
    private $id;

    /**
     * @var string $name
     *
     * @Column(name="name", type="string", length=50, nullable=true)
     */
    private $name;


}